{
    "version": 2,
    "builds": [
        { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "^/static/(.*)", "dest": "/static/$1" },
        { "src": "/(.*\\.js)", "dest": "/$1" },
        { "src": "/(.*\\.css)", "dest": "/$1" },
        { "src": "/(.*)", "dest": "/index.html" }
    ],
    "framework": "none"
}